{
    "name": "pedroresende/symfonyfeaturetoggle",
    "description": "Symfony's Feature Toggle Bundle",
    "type": "symfony-bundle",
    "keywords": [
        "php",
        "insights",
        "console",
        "quality",
        "source",
        "code"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Resende",
            "email": "pedroresende@mail.resende.biz"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^2.0",
        "pestphp/pest": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "PedroResende\\SymfonyFeatureToggle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
